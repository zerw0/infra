# vim: set ft=yaml:
global:
    scrape_interval: 5s
    scrape_timeout: 5s

scrape_configs:
    - job_name: services
      metrics_path: /metrics
      static_configs:
          - targets:
                - 'localhost:9090'
    
    - job_name: 'node_exporter'
      static_configs:
          - targets: ['10.0.1.10:9100']